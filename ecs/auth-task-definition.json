{
  "containerDefinitions": [
    {
      "cpu": 0,
      "environment": [
        {
          "name": "ASPNETCORE_URLS",
          "value": "http://0.0.0.0:8080"
        }
      ],
      "environmentFiles": [],
      "essential": true,
      "image": "886436922604.dkr.ecr.eu-west-2.amazonaws.com/regulator/auth-service@sha256:0b8c17b6cc2fb8c98a86307beec8b0fd9586cce1e05bef5960df4dbbe4264992",
      "mountPoints": [],
      "name": "auth-service",
      "portMappings": [
        {
          "appProtocol": "http",
          "containerPort": 8080,
          "hostPort": 8080,
          "name": "8080",
          "protocol": "tcp"
        }
      ],
      "systemControls": [],
      "ulimits": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/auth-service",
          "awslogs-region": "eu-west-2",
          "awslogs-stream-prefix": "auth-service"
        }
      },
      "secrets": [
        {
          "name": "Authentication__Discord__ClientId",
          "valueFrom": "arn:aws:ssm:eu-west-2:886436922604:parameter/auth-service/discord/client-id"
        },
        {
          "name": "Authentication__Discord__ClientSecret",
          "valueFrom": "arn:aws:ssm:eu-west-2:886436922604:parameter/auth-service/discord/client-secret"
        }
      ],
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:8080/health || exit 1"
        ],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 10
      }
    }
  ],
  "cpu": "256",
  "executionRoleArn": "arn:aws:iam::886436922604:role/ecsTaskExecutionRole",
  "family": "auth-service",
  "memory": "256",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "EC2"
  ],
  "revision": 1,
  "runtimePlatform": {
    "cpuArchitecture": "ARM64",
    "operatingSystemFamily": "LINUX"
  },
  "volumes": [],
  "tags": []
}