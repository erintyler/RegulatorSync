{
  "containerDefinitions": [
    {
      "environmentFiles": [],
      "essential": true,
      "image": "886436922604.dkr.ecr.eu-west-2.amazonaws.com/regulator/files-service@sha256:0b8c17b6cc2fb8c98a86307beec8b0fd9586cce1e05bef5960df4dbbe4264992",
      "mountPoints": [],
      "name": "files-service",
      "portMappings": [
        {
          "appProtocol": "http",
          "containerPort": 8080,
          "hostPort": 0,
          "name": "8080",
          "protocol": "tcp"
        }
      ],
      "systemControls": [],
      "ulimits": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/files-service",
          "awslogs-region": "eu-west-2",
          "awslogs-stream-prefix": "files-service"
        }
      },
      "secrets": [
        {
          "name": "TokenSettings__Audience",
          "valueFrom": "arn:aws:ssm:eu-west-2:886436922604:parameter/shared/token/audience"
        },
        {
          "name": "TokenSettings__AccessTokenExpirationMinutes",
          "valueFrom": "arn:aws:ssm:eu-west-2:886436922604:parameter/shared/token/expiration-minutes"
        },
        {
          "name": "TokenSettings__Issuer",
          "valueFrom": "arn:aws:ssm:eu-west-2:886436922604:parameter/shared/token/issuer"
        },
        {
          "name": "TokenSettings__RefreshTokenExpirationDays",
          "valueFrom": "arn:aws:ssm:eu-west-2:886436922604:parameter/shared/token/refresh-expiration-days"
        },
        {
          "name": "TokenSettings__Secret",
          "valueFrom": "arn:aws:ssm:eu-west-2:886436922604:parameter/shared/token/secret"
        }
      ],
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:8080/health || exit 1"
        ],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 10
      }
    }
  ],
  "cpu": "512",
  "executionRoleArn": "arn:aws:iam::886436922604:role/ecsTaskExecutionRole",
  "family": "files-service",
  "memory": "128",
  "networkMode": "bridge",
  "requiresCompatibilities": [
    "EC2"
  ],
  "revision": 1,
  "runtimePlatform": {
    "cpuArchitecture": "ARM64",
    "operatingSystemFamily": "LINUX"
  },
  "volumes": [],
  "tags": []
}